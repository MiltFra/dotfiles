{
    "window.menuBarVisibility": "toggle",
    "workbench.iconTheme": "vscode-icons",
    "material-icon-theme.showWelcomeMessage": false,
    "editor.fontFamily": "Input Mono",
    "editor.fontSize": 20,
    "latex-workshop.latex.tools": [
        {
            "name": "lualatex",
            "command": "lualatex",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "%DOC%"
            ]
        },
        {
            "name": "pdflatex",
            "command": "pdflatex",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "%DOC%"
            ]
        },
        {
            "name": "bibtex",
            "command": "bibtex",
            "args": [
                "%DOCFILE%"
            ]
        }
    ],
    "latex-workshop.latex.recipes": [
        {
            "name": "lualatex",
            "tools": [
                "lualatex",
                "lualatex"
            ]
        },
        {
            "name": "lualatex -> bibtex -> lualatex*2",
            "tools": [
                "lualatex",
                "bibtex",
                "lualatex",
                "lualatex"
            ]
        },
    ],
    "editor.minimap.enabled": false,
    "explorer.openEditors.visible": 0,
    "latex-workshop.message.update.show": false,
    "latex-workshop.view.pdf.viewer": "tab",
    "explorer.confirmDelete": false,    
    "explorer.confirmDragAndDrop": false,
    "editor.fontLigatures": true,
    "color-highlight.markerType": "outline",
    "workbench.sideBar.location": "left",
    "terminal.integrated.rendererType": "dom",
    "terminal.integrated.fontSize": 18,
    "workbench.activityBar.visible": true,
    "workbench.editor.openPositioning": "left",
    "search.location": "sidebar",
    "latex-workshop.showContextMenu": true,
    "zenMode.restore": false,
    "zenMode.centerLayout": false,
    "languageServerHaskell.useCustomHieWrapper": true,
    "languageServerHaskell.useCustomHieWrapperPath": "/home/miltfra/.local/bin/hie-wrapper",
    "code-runner.runInTerminal": true,
    "window.zoomLevel": 0,
    "bracket-pair-colorizer-2.colors": [
        "#c3b5d3",
        "#88898f",
        "#5e6173"
    ],
    "window.titleBarStyle": "native",
    "python.formatting.provider": "yapf",
    "gitlens.menus": false,
    "gitlens.blame.heatmap.enabled": false,
    "gitlens.blame.highlight.enabled": false,
    "gitlens.hovers.enabled": false,
    "gitlens.heatmap.toggleMode": "window",
    "explorer.sortOrder": "type",
    "bracketPairColorizer.consecutivePairColors": [
        "()",
        "[]",
        "{}",
        [
            "#dddddd",
            "#bbbbbb",
            "#999999",
            "#777777"
        ],
        "Red"
    ],
    "workbench.colorTheme": "lucy",
    "vim.useSystemClipboard": true,
    "vim.hlsearch": true,
    "editor.cursorStyle": "underline",
    "vim.useCtrlKeys": false,
    "vim.statusBarColorControl": true,
    "vim.statusBarColors.normal": "#0e0f15",
    "vim.statusBarColors.insert": "#c5c5c5",
    "vim.statusBarColors.visual": "#14161e",
    "vim.statusBarColors.visualline": "#B48EAD",
    "vim.statusBarColors.visualblock": "#A3BE8C",
    "vim.statusBarColors.replace": "#D08770",
    "workbench.colorCustomizations": {
        "statusBar.background": "#0e0f15",
        "statusBar.noFolderBackground": "#0e0f15",
        "statusBar.debuggingBackground": "#0e0f15"
    },
    "better-comments.tags": [
        {
            "tag": "!",
            "color": "#d31d5d",
            "strikethrough": false,
            "backgroundColor": "transparent"
        },
        {
            "tag": "todo",
            "color": "#1d5dd3",
            "strikethrough": false,
            "backgroundColor": "transparent"
        },
        {
            "tag": "//",
            "color": "#474747",
            "strikethrough": true,
            "backgroundColor": "transparent"
        },
        {
            "tag": "?",
            "color": "#5dd31d",
            "strikethrough": false,
            "backgroundColor": "transparent"
        },
        {
            "tag": "*",
            "color": "#98C379",
            "strikethrough": false,
            "backgroundColor": "transparent"
        }
    ]
}